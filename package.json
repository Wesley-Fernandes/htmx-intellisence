{
  "name": "htmx-intellisensce",
  "displayName": "Htmx Intellisence",
  "publisher": "wesley-fernandes",
  "version": "1.0.5",
  "icon": "images/logo.png",
  "keywords": ["Htmx", "htmx", "intellisensce", "snippets", "WesleyFernandes"],
  "license": "ISC",
  "engines": {
    "vscode": "^1.22.0"
  },
  "description": "Intellisence HTMX para HTML.",
  "contributes": {
    "snippets": [
      {
      "language": "html",
      "path": "./snippets/htmx-core-html.json"
    },
    {
      "language": "html",
      "path": "./snippets/htmx-add-html.json"
    }
    ]
  },
  "repository": {
    "url": "https://github.com/Wesley-Fernandes/htmx-intellisence",
    "type": "github"
  },
  "scripts": {
    "build": "vsce package -o htmx-intellisensce-1.0.5.vsix",
    "install": "npm run build && code --install-extension htmx-intellisensce-1.0.5.vsix"
  }
  
}
