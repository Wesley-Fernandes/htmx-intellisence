{
  "hx-confirm": {
      "prefix": "hx-confirm",
      "body": [
          "hx-confirm=\"${01}\""
      ],
      "description": [
          "exibe uma caixa de diálogo de confirmação antes de emitir uma requisição",
          "https://htmx.org/attributes/hx-confirm/"
      ]
  },
  "hx-delete": {
      "prefix": "hx-delete",
      "body": [
          "hx-delete=\"/${1:ENDPOINT}\""
      ],
      "description": [
          "emite uma requisição DELETE para a URL especificada",
          "https://htmx.org/attributes/hx-delete/"
      ]
  },
  "hx-disable": {
      "prefix": "hx-disable",
      "body": [
          "hx-disable"
      ],
      "description": [
          "desabilita o processamento htmx para o nó dado e quaisquer nós filhos",
          "https://htmx.org/attributes/hx-disable/"
      ]
  },
  "hx-disabled-elt": {
      "prefix": "hx-disabled-elt",
      "body": [
          "hx-disabled-elt=\"${1:this}\""
      ],
      "description": [
          "adiciona o atributo disabled aos elementos especificados enquanto uma requisição está em andamento",
          "https://htmx.org/attributes/hx-disabled-elt/"
      ]
  },
  "hx-disinherit": {
      "prefix": "hx-disinherit",
      "body": [
          "hx-disinherit=\"*\""
      ],
      "description": [
          "controla e desabilita a herança automática de atributos para nós filhos",
          "https://htmx.org/attributes/hx-disinherit/"
      ]
  },
  "hx-encoding": {
      "prefix": "hx-encoding",
      "body": [
          "hx-encoding"
      ],
      "description": [
          "altera o tipo de codificação da requisição",
          "https://htmx.org/attributes/hx-encoding/"
      ]
  },
  "hx-ext": {
      "prefix": "hx-ext",
      "body": [
          "hx-ext=\"${01}\""
      ],
      "description": [
          "extensões a serem usadas para este elemento",
          "https://htmx.org/attributes/hx-ext/"
      ]
  },
  "hx-headers": {
      "prefix": "hx-headers",
      "body": [
          "hx-headers='{\"${1:myHeader}\": \"${2:My Value}\"}'"
      ],
      "description": [
          "adiciona aos cabeçalhos que serão enviados com a requisição",
          "https://htmx.org/attributes/hx-headers/"
      ]
  },
  "hx-history": {
      "prefix": "hx-history",
      "body": [
          "hx-history=\"${1:false}\""
      ],
      "description": [
          "impede que dados sensíveis sejam salvos no cache do histórico",
          "https://htmx.org/attributes/hx-history/"
      ]
  },
  "hx-history-elt": {
      "prefix": "hx-history-elt",
      "body": [
          "hx-history-elt"
      ],
      "description": [
          "o elemento a ser capturado e restaurado durante a navegação do histórico",
          "https://htmx.org/attributes/hx-history-elt/"
      ]
  },
  "hx-include": {
      "prefix": "hx-include",
      "body": [
          "hx-include=\"[${1:name}='${2:email}']\""
      ],
      "description": [
          "inclui dados adicionais nas requisições",
          "https://htmx.org/attributes/hx-include/"
      ]
  },
  "hx-indicator": {
      "prefix": "hx-indicator",
      "body": [
          "hx-indicator=\"${01}\""
      ],
      "description": [
          "o elemento para colocar a classe htmx-request durante a requisição",
          "https://htmx.org/attributes/hx-indicator/"
      ]
  },
  "hx-indicator-class": {
      "prefix": "hx-indicator-class",
      "body": [
          "class=\"htmx-indicator\""
      ],
      "description": [
          "Adiciona a classe htmx-indicator para indicadores de carregamento",
          "https://htmx.org/attributes/hx-indicator/"
      ]
  },
  "hx-params": {
      "prefix": "hx-params",
      "body": [
          "hx-params=\"*${01}\""
      ],
      "description": [
          "filtra os parâmetros que serão enviados com uma requisição",
          "https://htmx.org/attributes/hx-params/"
      ]
  },
  "hx-patch": {
      "prefix": "hx-patch",
      "body": [
          "hx-patch=\"/${1:ENDPOINT}\""
      ],
      "description": [
          "emite uma requisição PATCH para a URL especificada",
          "https://htmx.org/attributes/hx-patch/"
      ]
  },
  "hx-preserve": {
      "prefix": "hx-preserve",
      "body": [
          "hx-preserve"
      ],
      "description": [
          "especifica elementos a serem mantidos inalterados entre requisições",
          "https://htmx.org/attributes/hx-preserve/"
      ]
  },
  "hx-prompt": {
      "prefix": "hx-prompt",
      "body": [
          "hx-prompt=\"${01}\""
      ],
      "description": [
          "exibe um prompt() antes de enviar uma requisição",
          "https://htmx.org/attributes/hx-prompt/"
      ]
  },
  "hx-put": {
      "prefix": "hx-put",
      "body": [
          "hx-put=\"/${1:ENDPOINT}\""
      ],
      "description": [
          "emite uma requisição PUT para a URL especificada",
          "https://htmx.org/attributes/hx-put/"
      ]
  },
  "hx-replace-url": {
      "prefix": "hx-replace-url",
      "body": [
          "hx-replace-url=\"${01}\""
      ],
      "description": [
          "substitui a URL na barra de localização do navegador",
          "https://htmx.org/attributes/hx-replace-url/"
      ]
  },
  "hx-request": {
      "prefix": "hx-request",
      "body": [
          "hx-request='${01}'"
      ],
      "description": [
          "configura vários aspectos da requisição",
          "https://htmx.org/attributes/hx-request/"
      ]
  },
  "hx-sync": {
      "prefix": "hx-sync",
      "body": [
          "hx-sync=\"${01}\""
      ],
      "description": [
          "controla como as requisições feitas por diferentes elementos são sincronizadas",
          "https://htmx.org/attributes/hx-sync/"
      ]
  },
  "hx-validate": {
      "prefix": "hx-validate",
      "body": [
          "hx-validate"
      ],
      "description": [
          "força os elementos a se validarem antes de uma requisição",
          "https://htmx.org/attributes/hx-validate/"
      ]
  }
}